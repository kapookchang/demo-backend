    "amqplib": "^0.5.2",
    "body-parser": "^1.18.2",
    "lodash": "^4.17.4",
    "mongoose": "^4.13.4",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.5.2"
  },
  "devDependencies": {
    "@types/jest": "^21.1.6",
    "@types/mongoose": "^4.7.27",
